{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev --host",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare && pnpm db:push",
    "db:generate": "npx prisma generate --schema ./server/prisma/schema.prisma",
    "db:push": "npx prisma db push --schema ./server/prisma/schema.prisma && pnpm db:generate && pnpm db:seed",
    "db:seed": "npx prisma db seed --schema ./server/prisma/schema.prisma",
    "db:studio": "npx prisma studio --schema ./server/prisma/schema.prisma",
    "db:migrate": "npx prisma migrate dev --schema ./server/prisma/schema.prisma"
  },
  "devDependencies": {
    "@fontsource-variable/inter": "^5.0.18",
    "@formkit/nuxt": "^1.6.5",
    "@iconify-json/mingcute": "^1.1.17",
    "@iconify-json/svg-spinners": "^1.1.2",
    "@iconify-json/tabler": "^1.1.113",
    "@nuxt/devtools": "^1.3.3",
    "@nuxt/image": "^1.7.0",
    "@nuxtjs/fontaine": "^0.4.2",
    "@pinia/nuxt": "^0.5.1",
    "@types/nodemailer": "^6.4.15",
    "@vueuse/nuxt": "^10.10.0",
    "esrun": "^3.2.26",
    "nuxt": "^3.11.2",
    "nuxt-tiptap-editor": "^1.1.2",
    "prisma": "^5.14.0",
    "typescript": "^5.4.5",
    "vue": "^3.4.27",
    "vue-router": "^4.3.2"
  },
  "dependencies": {
    "@firebase/app-types": "^0.9.2",
    "@lucia-auth/adapter-prisma": "^3.0.2",
    "@nuxt/ui": "^2.16.0",
    "@prisma/client": "^5.15.0",
    "@tiptap/extension-placeholder": "^2.4.0",
    "@tiptap/extension-underline": "^2.4.0",
    "@unocss/nuxt": "^0.60.4",
    "@vue-email/nuxt": "^0.8.19",
    "firebase": "^10.12.2",
    "firebase-admin": "^12.1.1",
    "firebase-functions": "^5.0.1",
    "h3-valibot": "^0.0.7",
    "lucia": "^2.7.7",
    "nodemailer": "^6.9.13",
    "notivue": "^2.4.4",
    "pinia": "^2.1.7",
    "swiper": "^11.1.4",
    "uncrypto": "^0.1.3",
    "valibot": "^0.31.0"
  },
  "prisma": {
    "seed": "esrun ./server/prisma/seed.ts"
  }
}
